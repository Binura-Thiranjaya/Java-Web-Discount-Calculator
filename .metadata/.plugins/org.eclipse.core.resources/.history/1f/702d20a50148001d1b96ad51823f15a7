package controller;
import model.DiscountModel;
import databaseAccessLayer.DiscountAccessLogic;


public class DiscountController {
	DiscountModel objModel;
	DiscountAccessLogic objAccessLogic;
	
	public DiscountController() {
		objAccessLogic = new DiscountAccessLogic();
	}
	public DiscountModel setCustomerId(int id) {
		objModel = new  DiscountModel(id);
		return objModel
	}
}
